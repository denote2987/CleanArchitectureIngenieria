{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "Server": "(localdb)\\MSSQLLocalDB",
    "Database": "InventarioDb",
    "UseIntegratedSecurity": true,
    "ConnectTimeout": 30,
    "Encrypt": true,
    "TrustServerCertificado": false,
    "ApplicationIntent": "ReadWrite",
    "MultiSubnetFailover": false,
    "CommandTimeout": 30
  }
}
